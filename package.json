    "dotenv": "^14.3.2",
    "esbuild": "^0.12.7",
    "eslint": "^7.28.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.1",
    "hardhat": "^2.4.1",
    "mocha": "^11.1.0",
    "node-fetch": "^2.6.1",
    "nyc": "^15.1.0",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.7",
    "web3-provider-engine": "^16.0.1"
